<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Newsletter Signup!</title>
    <link rel="stylesheet" href="/public/css/style.css">
    <script src="/public/scripts/scripts.js" defer></script>
</head>
<body>
    <section>

        <% const formData = form.formData; %>
        <% const formFirst = form.formFirst; %> 
        <% const formLast = form.formLast; %>
        <% const formEmail = form.formEmail; %>
        <% const formText = form.formText; %>
        <% const formBtn = form.formBtn; %>
        
        <form
        <% for (const [key, value] of Object.entries(formData)) { %>
            <%= key %>="<%= value %>"
        <% } %>
        >   
        <input 
            <% for (const [key, value] of Object.entries(formFirst)) { %>
                <%= key %>="<%= value %>"
            <% } %>
        >
    
        <input 
            <% for (const [key, value] of Object.entries(formLast)) { %>
                <%= key %>="<%= value %>"
            <% }; %>
        >
                
        <input
            <% for (const [key, value] of Object.entries(formEmail)) { %>
                <%= key %>="<%= value %>"
            <% } %>
        >
    
        <input
            <% for (const [key, value] of Object.entries(formText)) { %>
                <%= key %>="<%= value %>"
            <% } %>
        >
    </form>
    
        


        </form>
    </section>
</body>
</html>
